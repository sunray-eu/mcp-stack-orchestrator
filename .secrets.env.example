# Archon profile (required for archon/full)
SUPABASE_URL=
SUPABASE_SERVICE_KEY=
HOST=localhost
LOG_LEVEL=INFO
GITHUB_PAT_TOKEN=

# Archon settings sync (applied automatically on stack_infra.sh up archon/full)
ARCHON_LLM_PROVIDER=openai
ARCHON_MODEL_CHOICE=gpt-5.2
ARCHON_EMBEDDING_PROVIDER=openai
ARCHON_EMBEDDING_MODEL=text-embedding-3-large
ARCHON_USE_AGENTIC_RAG=true
ARCHON_USE_HYBRID_SEARCH=true
ARCHON_USE_RERANKING=true
ARCHON_USE_CONTEXTUAL_EMBEDDINGS=true
